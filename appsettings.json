{
  "ApiUrls": {
    "MusicBrainz": "http://musicbrainz.org/ws/2/artist/{mbId}?&fmt=json&inc=url-rels+release-groups",
    "CoverArt": "http://coverartarchive.org/release-group/",
    "WikiData": "https://www.wikidata.org/w/api.php?action=wbgetentities&ids={id}&format=json&props=sitelinks",
    "Wikipedia": "https://en.wikipedia.org/w/api.php?action=query&format=json&prop=extracts&exintro=true&redirects=true&titles={title}"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
